{
	"macros": [
		"MBED_HEAP_STATS_ENABLED=1",
		"MBED_STACK_STATS_ENABLED=1",
		"MBED_MEM_TRACING_ENABLED=1",
		"NRFX_GPIOTE_CONFIG_NUM_OF_LOW_POWER_EVENTS=8",
		"NRFX_WDT_ENABLED=1"
	],
	"target_overrides": {
		"*": {
