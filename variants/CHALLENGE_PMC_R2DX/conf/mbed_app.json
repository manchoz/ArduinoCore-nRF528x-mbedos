			"sd.SPI_MISO": "P1_8",
			"sd.SPI_CLK": "P0_13",
			"sd.SPI_CS": "P1_15",
			"spif-driver.SPI_MOSI": "P1_1",
			"spif-driver.SPI_MISO": "P1_8",
			"spif-driver.SPI_CLK": "P0_13",
			"spif-driver.SPI_CS": "P1_13",
			"storage.storage_type": "TDB_EXTERNAL"
		}
	}
